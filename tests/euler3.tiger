let
   n = 600851475143
   i = 2

   prime(n: int): bool = let
      i = 2
      isprime = true
   in
      while i * i <= n and isprime do
         if n mod i = 0 then
            isprime := false
         i := i + (if i = 2 then 1 else 2)
      end
      
      isprime
   end
in
   while i * i <= n do
      if prime(i) then
         if n mod i = 0 then
            print(str(i))
      i := i + 1
   end
end


