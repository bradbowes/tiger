/*
   Write file to STD_OUTPUT. Use
   STD_INPUT if no file is specified.
*/

let
   input =
      if command_argcount() < 2 then STD_INPUT
      else open_input(command_getarg(1))
   ch =
      if input <> nil then
         getchar_file(input)
      else
         begin
            writeln("ERROR: can't open file!")
            EOF
         end
in
   while ch <> EOF do
      begin
         putchar(ch)
         ch := getchar_file(input)
      end

   close_file(input)
end
